  "description": "E-commerce COD Admin Dashboard Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "seed": "ts-node scripts/seed-comprehensive-data.ts",
    "seed:gl": "ts-node scripts/seed-gl-accounts.ts",
    "lint": "eslint . --ext .ts",
    "test:performance": "ts-node src/scripts/test-query-performance.ts",
    "db:verify-indexes": "psql $DATABASE_URL -f src/scripts/verify-indexes.sql"
  },
  "keywords": [
    "ecommerce",
    "cod",
    "admin",
    "dashboard"
  ],
